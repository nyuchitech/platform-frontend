{
  "name": "@nyuchi/database",
  "version": "1.0.0",
  "description": "Supabase database client and queries for Nyuchi Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "migrate": "node dist/migrations/run.js",
    "seed": "node dist/seeds/run.js",
    "generate-types": "supabase gen types typescript --local > src/types/database.ts"
  },
  "keywords": ["supabase", "database", "nyuchi", "postgres"],
  "dependencies": {
    "@nyuchi/ubuntu": "*",
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  }
}
